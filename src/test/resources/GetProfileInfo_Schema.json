{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "FirstName",
    "LastName",
    "UserRole"
  ],
  "properties": {
    "FirstName": {
      "$id": "#/properties/FirstName",
      "type": "string",
      "title": "The Firstname Schema",
      "default": "",
      "examples": [
        "Santosh"
      ],
      "pattern": "^(.*)$"
    },
    "LastName": {
      "$id": "#/properties/LastName",
      "type": "string",
      "title": "The Lastname Schema",
      "default": "",
      "examples": [
        "Malina"
      ],
      "pattern": "^(.*)$"
    },
    "UserRole": {
      "$id": "#/properties/UserRole",
      "type": "string",
      "title": "The Userrole Schema",
      "default": "",
      "examples": [
        "LEAD"
      ],
      "pattern": "^(.*)$"
    }
  }
}