{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "Id",
    "IsSession",
    "AttendanceStatus"
  ],
  "properties": {
    "Id": {
      "$id": "#/properties/Id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        43
      ]
    },
    "IsSession": {
      "$id": "#/properties/IsSession",
      "type": "boolean",
      "title": "The Issession Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "AttendanceStatus": {
      "$id": "#/properties/AttendanceStatus",
      "type": "string",
      "title": "The Attendancestatus Schema",
      "default": "",
      "examples": [
        "ATTENDENCE SUCCESSFULLY TAKEN"
      ],
      "pattern": "^(.*)$"
    }
  }
}